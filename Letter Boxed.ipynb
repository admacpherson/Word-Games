    "import re\n",
    "\n",
    "with open(\"/usr/share/dict/words\", \"r\") as file:\n",
    "    words = re.sub(r\"[^\\w]\", \" \", file.read()).split()\n",
    "\n",
    "def is_word(word):\n",
    "    return word.lower() in words"
   ]
  },
